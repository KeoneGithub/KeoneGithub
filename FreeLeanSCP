Button_10.TextSize = 14.000
Button_10.MouseButton1Down:connect(function()


	local name = "SCP-GAMING"
	local Plr = game.Players.LocalPlayer

	local function replaceNameInText(text)
		return text:gsub(Plr.Name, name)
	end

	local function handleTextLabel(textLabel)
		local hasName = string.find(textLabel.Text, Plr.Name)
		textLabel.Text = replaceNameInText(textLabel.Text)

		textLabel:GetPropertyChangedSignal("Text"):Connect(function()
			textLabel.Text = replaceNameInText(textLabel.Text)
		end)
	end

	local function onDescendantAdded(descendant)
		if descendant:IsA("TextLabel") then
			handleTextLabel(descendant)
		end
	end

	for _, textLabel in ipairs(game:GetDescendants()) do
		if textLabel:IsA("TextLabel") then
			handleTextLabel(textLabel)
		end
	end

	game.DescendantAdded:Connect(onDescendantAdded)
	--------------------
	--Spoof Username
	--------------------

end)

